{
    "search.exclude": {
        "**/node_modules": true,
        "**/bower_components": true,
        "**/*.o":true,
        "**/*.su":true, 
        "**/*.cmd":true,
        "Documentation":true,      
    },
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,  
        "**/*.o":true,
        "**/*.su":true, 
        "**/*.cmd":true,
        "Documentation":true, 
    },
    "files.associations": {
        "module.h": "c",
        "init.h": "c",
        "kernel.h": "c",
        "unistd.h": "c",
        "fcntl.h": "c",
        "uaccess.h": "c",
        "io.h": "c",
        "of.h": "c",
        "cdev.h": "c",
        "device.h": "c",
        "slab.h": "c",
        "type_traits": "c",
        "of_address.h": "c"
    },
    "C_Cpp.errorSquiggles": "disabled",
    "C_Cpp.codeAnalysis.clangTidy.checks.disabled": [
        "clang-diagnostic-error"
    ]
}